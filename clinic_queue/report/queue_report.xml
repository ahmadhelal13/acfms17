    <!-- =============dental===================== -->
    <data>
        <template id="dental_queue_report_template">
            <t t-call="clinic_queue.queue_template_page"/>
        </template>

        <record id="dental_queue_number_report_action" model="ir.actions.report">
            <field name="name">Dental Q No.</field>
            <field name="model">ksc.dental.appointment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">clinic_queue.dental_queue_report_template</field>
            <field name="report_file">clinic_queue.dental_queue_report_template</field>
            <field name="binding_model_id" ref="model_ksc_appointment" />
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="paperformat_thermal_custom"/>
        </record>
    </data>

