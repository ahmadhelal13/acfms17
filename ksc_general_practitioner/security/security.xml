<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ksc_practitioner_category" model="ir.module.category">
            <field name="name">practitioner</field>
        </record>

        <record id="ksc_practitioner_nurse" model="res.groups">
            <field name="name">Nurse</field>
            <field name="category_id" ref="ksc_practitioner_category"/>
        </record>

        <record id="ksc_practitioner_doctor" model="res.groups">
            <field name="name">Doctor</field>
            <field name="category_id" ref="ksc_practitioner_category"/>
        </record>

        <record id="ksc_practitioner_receptionist" model="res.groups">
            <field name="name">Receptionist</field>
            <field name="category_id" ref="ksc_practitioner_category"/>
        </record>

        <record id="ksc_practitioner_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="ksc_practitioner_category"/>
        </record>
    </data>
</odoo>
