<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="lab_parameter_line_view_form" model="ir.ui.view">
        <field name="name">lab.parameter.line.view.form</field>
        <field name="model">lab.parameter.line</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="gender"/>
                            <field name="from_age"/>
                            <field name="to_age"/>
                            <field name="from_by_day"/>
                            <field name="to_by_day"/>
                            <field name="age_type"/>
                            <field name="test_id"/>
                            <field name="lab_uom_id"/>
                            <field name="test_type" force_save="1" invisible="1"/>
                        </group>
                        <group>
                            <field name="normal_range"/>
                            <field name="remark"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="lab_parameter_line_view_tree" model="ir.ui.view">
        <field name="name">lab.parameter.line.view.tree</field>
        <field name="model">lab.parameter.line</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="gender"/>
                <field name="from_age"/>
                <field name="to_age"/>
                <field name="age_type"/>
                <field name="test_id"/>
                <field name="lab_uom_id"/>
                <field name="normal_range"/>
                <field name="remark"/>

            </tree>
        </field>
    </record>

    <record id="lab_parameter_view_form" model="ir.ui.view">
        <field name="name">lab.parameter.view.form</field>
        <field name="model">lab.parameter</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="lab_uom_id" invisible="not custom_unit"/>
                        </group>
                        <group>
                            <field name="test_type"/>
                        </group>
                        <group>
                            <field name="structure_selection"/>
                            <field name="is_parameter" invisible="1"/>
                            <field name="is_customize" invisible="1"/>
                            <field name="custom_unit"/>
                        </group>
                        <group invisible="is_parameter">
                            <field name="custom_reference"/>
                            <field name="custom_polar"/>
                            <field name="custom_time"/>
                        </group>
                    </group>
