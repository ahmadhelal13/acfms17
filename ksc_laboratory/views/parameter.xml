    <record id="lab_parameter_line_two_view_form" model="ir.ui.view">
        <field name="name">lab.parameter.line.view.form</field>
        <field name="model">lab.parameter.line.two</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>

                            <field name="pr2_gender"/>
                            <field name="pr2_from_age"/>
                            <field name="pr2_to_age"/>

                            <field name="pr2_from_by_day"/>
                            <field name="pr2_to_by_day"/>
                            <field name="pr2_age_type"/>

                            <field name="test_id"/>
                            <field name="pr2_lab_uom_id"/>
                            <field name="test_type" force_save="1" invisible="1"/>


                        </group>
                        <group>
                            <field name="pr_normal_range"/>
                            <field name="remark"/>
                        </group>

                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="lab_parameter_line_two_view_tree" model="ir.ui.view">
        <field name="name">lab.parameter.line.view.tree</field>
        <field name="model">lab.parameter.line.two</field>
        <field name="arch" type="xml">
