                <group name="group_by" string="Group By">
                    <filter name="group_by_test_type" string="Test Type" context="{'group_by': 'test_type'}"/>
                </group>
            </search>
        </field>
    </record>
    <record id="lab_parameter_line_two_action" model="ir.actions.act_window">
        <field name="name">Parameter Lines Two</field>
        <field name="res_model">lab.parameter.line.two</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Record Found
            </p>
            <p>
                Click to add new parameter line two.
            </p>
        </field>
    </record>

