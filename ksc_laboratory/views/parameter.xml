                    <!-- attrs="{'column_invisible': [('parent.is_customize', '!=', False)]}"/>-->
                    <!--                                <field name="from_by_day" groups="base.group_no_one"-->
                    <!-- attrs="{'column_invisible': [('parent.is_customize', '!=', False)]}"/>-->
                    <!--                                <field name="to_by_day" groups="base.group_no_one"-->
                    <!-- attrs="{'column_invisible': [('parent.is_customize','!=',False)]}"/>-->
                    <!--                                <field name="normal_range"-->
                    <!-- attrs="{'column_invisible': [('parent.is_customize','!=',False)]}"/>-->
                    <!--                                <field name="cust_polar_input"-->
                    <!-- attrs="{'column_invisible': [('parent.custom_polar', '=', False)]}"/>-->
                    <!--                                <field name="cust_ref_input"-->
                    <!-- attrs="{'column_invisible': [('parent.custom_reference', '=', False)]}"/>-->
                    <!--                                <field name="remark"/>-->
                    <!--                            </tree>-->
                    <!--                        </field>-->
                    <!--                    </group>-->
                </sheet>
            </form>
        </field>
    </record>

    <record id="lab_parameter_view_tree" model="ir.ui.view">
        <field name="name">lab.parameter.view.tree</field>
        <field name="model">lab.parameter</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="lab_uom_id"/>
            </tree>
        </field>
    </record>


    <record id="lab_parameter_action" model="ir.actions.act_window">
        <field name="name">Parameters</field>
        <field name="res_model">lab.parameter</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Record Found
            </p>
            <p>
                Click to add new parameter.
            </p>
        </field>
    </record>
    <record id="paramter_line_view_search" model="ir.ui.view">
        <field name="name">paramter_line.view.search</field>
        <field name="model">lab.parameter.line</field>
        <field name="arch" type="xml">
            <search string="">
                <filter name="test_type"/>
                <group name="group_by" string="Group By">
                    <filter name="group_by_test_type" string="Test Type" context="{'group_by': 'test_type'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id="lab_parameter_line_action" model="ir.actions.act_window">
        <field name="name">Parameter Lines</field>
        <field name="res_model">lab.parameter.line</field>
        <field name="view_mode">tree,form</field>
