<odoo>

    <record model="ir.module.category" id="module_product_transfer">
        <field name="name">Transfer</field>
        <field name="sequence">12</field>
    </record>

    <record id="group_transfer_users" model="res.groups">
        <field name="name">Transfer User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="category_id" ref="module_product_transfer"/>
    </record>
    <record id="ir_rule_state_only_docs" model="ir.rule">
        <field name="name">Own Only Document</field>
        <field ref="model_product_transfer_internal" name="model_id"/>
        <field name="domain_force">[('user_id.id','=',user.id)]</field>
        <field name="groups" eval="[(4, ref('group_transfer_users'))]"/>
    </record>

    <record id="group_transfer_manager" model="res.groups">
        <field name="name">Transfer Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_transfer_users'))]"/>
        <field name="category_id" ref="module_product_transfer"/>
    </record>
    <record id="ir_rule_state_manager_docs" model="ir.rule">
        <field name="name">Transfer Manager Rule</field>
        <field ref="model_product_transfer_internal" name="model_id"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('group_transfer_manager'))]"/>
    </record>



</odoo>